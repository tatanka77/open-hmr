		<itemDisplay>
			<image offsetXPC="0" offsetYPC="0" widthPC="100" heightPC="100">
				<script>
					idx1 = getFocusItemIndex();
					idx2 = getQueryItemIndex();
					if (idx1 == idx2)
						"../etc/translate/rss/image/IMAGE_NEWRSS_MENU_BTFOC.bmp";
					else
						"";
				</script>
			</image>

			<text offsetXPC="0" offsetYPC="25" widthPC="100" heightPC="50" fontSize="17" backgroundColor="-1:-1:-1" foregroundColor="200:200:200">
				<script>getStringArrayAt(tagsArray , -1);</script>
			</text>
		</itemDisplay>

		<onUserInput>
			<script>
				ret = "false";
				userInput = currentUserInput();
				print("*** userInput=",userInput);

				if (userInput == "pagedown" || userInput == "pageup")
				{
					idx = Integer(getFocusItemIndex());
					if (userInput == "pagedown")
					{
						idx -= -8;
						if(idx &gt;= itemSize)
							idx = itemSize-1;
					}
					else
					{
						idx -= 8;
						if(idx &lt; 0)
							idx = 0;
					}
					setFocusItemIndex(idx);
					setItemFocus(idx);
					redrawDisplay();
					ret = "true";
				}
				else if (userInput == "right")
					ret = "true";
				ret;
			</script>
		</onUserInput>
	</mediaDisplay>

